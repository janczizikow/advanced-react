{
  "env": {
    "development": {
      "presets": [ "next/babel" ],
      "plugins": [ [ "emotion", { "sourceMap": true, "autoLabel": true, "inline": true } ] ]
    },
    "production": {
      "presets": [ "next/babel" ],
      "plugins": [ [ "emotion", { "hoist": true, "inline": true } ] ]
    },
    "test": {
      "presets": [ [ "next/babel", { "preset-env": { "modules": "commonjs" } } ] ],
      "plugins": [ [ "emotion", { "inline": true } ] ]
    }
  }
}